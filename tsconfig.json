{
    "compilerOptions": {
        "target": "es5", //  es6 console写入tex会报错
        "jsx": "react",
        "module": "ESNext",
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "strict": true,
        "skipLibCheck": true,
        "moduleResolution": "node",
        "baseUrl": "./",
        "paths": {
            "utils/*": [
                "src/utils/*"
            ],
            "components/*": [
                "src/*"
            ],
        },
    },
    "include": [
        "src",
        "globals.d.ts"
    ],
    "exclude": [
        "node_modules",
        "dist"
    ]
}